licenses(["notice"])  # Apache 2

# This is a plain cc rule to ensure that the file is getting compiled as a plain C file, and provides plain C types.
cc_library(
    name = "c_types_lib",
    srcs = [
        "c_types.c",
    ],
    hdrs = [
        "c_types.h",
    ],
    visibility = ["//visibility:public"],
)
